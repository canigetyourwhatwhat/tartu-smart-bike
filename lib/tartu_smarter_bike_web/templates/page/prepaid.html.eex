<%= form_for @changeset, Routes.page_path(@conn, :prepaid), fn f -> %>
  <%= if @changeset.action do %>
    <div class="alert alert-danger">
      <p>Oops, something went wrong! Please check the errors below.</p>
    </div>
  <% end %>

<p>
    <%= label f, :prepaid_card_number %>
    <br>
    <%= text_input f, :prepaid_card_number, id: "prepaid_card_number" %>
    <%= error_tag f, :prepaid_card_number %>
</p>

  <div>
      <%= submit "Top-up", id: "submit_register" %>
  </div>
<% end %>
